#include <stdio.h>
#include <stdlib.h>


int main()
{
 int factNum, result, cont;

 printf("Ingrese el primer numero: \n");
 scanf("%d", &factNum);
 cont = 1;
 result = 1;
 while (cont <= factNum)
 {
    result = result * cont;
    cont++;
 }
 printf("El factorial de %d es: %d",factNum, result);

return 0;
}
